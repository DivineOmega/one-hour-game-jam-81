pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
scene = {}
ents = {}

function _init()
 init_game_scene()
end

function _update()
 scene.update()
end

function _draw()
 scene.draw()
end

function init_game_scene()
 palt(0, false)
 palt(12, true)
 
 ents = {}
 
 for i=1,3 do
  add(ents, create_berry_bush())
 end
 
 add(ents, create_girl())
 
 add(ents, create_hud())
 
 scene.update = game_update
 scene.draw = game_draw
end

function game_update()
 for ent in all(ents) do
  ent.update()
 end
end

function game_draw()
 rectfill(0, 0, 127, 127, 3)
 for ent in all(ents) do
  ent.draw()
 end
end

function create_girl()
 local ent = {}
 ent.t = "girl"
 ent.x = 50
 ent.y = 50
 ent.w = 15
 ent.h = 16
 ent.sprite = 0
 ent.spd = 1
 ent.health = 100
 
 ent.draw = function()
  spr(ent.sprite, ent.x, ent.y, 2, 2)
 end
 
 ent.update = function()
  if (btn(2)) then
   ent.y -= ent.spd
  elseif (btn(3)) then
   ent.y += ent.spd
  end
  if (btn(0)) then
   ent.x -= ent.spd
  elseif (btn(1)) then
   ent.x += ent.spd
  end
  
  if (ent.x<0) then
   ent.x = 0
  end
  if (ent.y<0+11) then
   ent.y = 0+11
  end
  if (ent.x>128-ent.w) then
   ent.x = 128-ent.w
  end
  if (ent.y>128-ent.h) then
   ent.y = 128-ent.h
  end
  
  for collider in all (ents) do
   if (collider.t == "berry_bush") then
    if (collision(ent, collider)) then
     ent.health += 5
     sfx(0)
     collider.rand_pos()
    end   
   end
  end
  
  ent.health -= 0.2
  
  if (ent.health <= 0) then
   sfx(1)
   init_game_scene()
  end
  
 end
 
 return ent
end

function create_berry_bush()
 local ent = {}
 ent.t = "berry_bush"
 ent.x = 0
 ent.y = 0
 ent.w = 8
 ent.h = 8
 ent.sprite = 2
 
 ent.draw = function()
	 spr(ent.sprite, ent.x, ent.y, 1, 1)
 end
 
 ent.update = function()

 end
 
 ent.rand_pos = function()
  ent.x = rnd(128-ent.w)
  ent.y = 11+rnd(128-11-ent.h)
 end
 
 ent.rand_pos()
 
 return ent
end

function collision(obj1, obj2)
 if (obj1.x < obj2.x+obj2.w) 
 and (obj2.x < obj1.x+obj1.w) 
 and (obj1.y < obj2.y+obj2.h) 
 and (obj2.y < obj1.y+obj1.h) then
  return true
 end
 return false
end

function create_hud()
 local ent = {}
 ent.t = "hud"
 ent.x = 0
 ent.y = 0
 ent.w = 127
 ent.h = 10
 ent.girl_health = 0
 ent.timer = 0
 ent.timer_high = 0
 
 ent.draw = function()
  rectfill(ent.x, ent.y, ent.w, ent.h, 0)
  print("‡: "..flr(ent.girl_health), 5, 5, 8)
  print("“: "..flr(ent.timer), 50, 5, 7)
  print("high: "..flr(ent.timer_high), 90, 5, 7)
 end
 
 ent.update = function()
  for test_ent in all(ents) do
   if (test_ent.t == "girl") then
    ent.girl_health = test_ent.health
   end
  end
  
  ent.timer += 0.03
  
  if (ent.timer_high < ent.timer) then
   ent.timer_high = ent.timer
   dset(0, ent.timer_high)
  end
  
 end
 
 function load_timer_high()
  ent.timer_high = dget(0)
 end
 
 load_timer_high()
 
 return ent
end
